@model IEnumerable<ProjectDB.Download>

@{
    ViewBag.Title = "MySoldNotes";
    Layout = "~/Views/Shared/_RagisterView.cshtml";
}

<!-- My Sold notes -->
<section id="my-sold-notes">

    <div class="content-box">
        <div class="container">
            <div class="row">
                <div class="col-md-6">
                    <div class="left-heading">
                        <h2> My Sold Notes </h2>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="search">
                        <div class="row">
                            <div class="col-md-7">
                                <div class="search-bar">
                                    <input type="text" class="form-control" id="search-input" aria- placeholder="        search">
                                </div>
                                <div class="search-icon">
                                    <img src="/Images/search-icon.png" alt="Search Icon" title="Search">
                                </div>
                            </div>
                            <div class="col-md-5">
                                <div class="search-btn">
                                    <button class="btn blue-btn" title="Search" onclick="getSoldNotes()" role="button"> SEARCH </button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-12">
                    <div id="getSold"></div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- My Sold Notes Ends -->
